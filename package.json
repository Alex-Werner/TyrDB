{
  "name": "tyrdb",
  "version": "3.1.1",
  "description": "Fast document oriented NodeJS database for node and browser. Allow adapters for in-memory, fs (JSON based or optimized), localStorage.",
  "main": "index.js",
  "scripts": {
    "serve:docs": "docsify serve ./docs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:unit": "nyc mocha 'src/**/*.spec.js'",
    "test:functional": "mocha 'test/functional/*.js'",
    "test:benchmark": "mocha test/benchmark --recursive",
    "test": "npm run test:unit && npm run test:functional"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alex-Werner/TyrDB.git"
  },
  "author": "Alex Werner <obusco@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Alex-Werner/TyrDB/issues"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "database",
    "in-memory",
    "javascript",
    "json",
    "document-oriented",
    "nosql",
    "tyrdb",
    "file",
    "storage",
    "local",
    "embeddable",
    "portable db"
  ],
  "homepage": "https://github.com/Alex-Werner/TyrDB#readme",
  "dependencies": {
    "eventemitter2": "^6.4.9",
    "fslockjs": "^2.0.2",
    "lodash.clone": "^4.5.0",
    "lodash.find": "^4.6.0",
    "lodash.foreach": "^4.5.0",
    "lodash.matches": "^4.6.0",
    "lodash.reduce": "^4.6.0",
    "mongo-objectid": "^1.2.2",
    "sbtree": "^3.1.1"
  },
  "devDependencies": {
    "chai": "^4.3.10",
    "chance": "^1.1.11",
    "eslint": "^8.51.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.28.1",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0t"
  }
}
